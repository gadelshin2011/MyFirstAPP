<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".passwordFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center">

        <Button
            android:id="@+id/buttonOpenAddNewData"
            android:layout_width="@dimen/widthButton"
            android:layout_height="@dimen/heightButton"
            android:layout_gravity="center_horizontal"
            android:text="@string/AddNewData"
            android:onClick="onClickOpenFragmentAddData"


            />

    </LinearLayout>

</FrameLayout>